    private void txtnasilemakKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtnasilemakKeyTyped
        // TODO add your handling code here:
        char iNumber = evt.getKeyChar();
        if(!(Character.isDigit(iNumber))
            || (iNumber == KeyEvent.VK_BACK_SPACE)
            || (iNumber == KeyEvent.VK_DELETE)){
        evt.consume();
    }
    }//GEN-LAST:event_txtnasilemakKeyTyped

    private void txtroticanaiKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txtroticanaiKeyTyped
