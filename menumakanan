    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // calculation happen here
        itemcost[0] = Double.parseDouble(txtnasilemak.getText());
        itemcost[1] = Double.parseDouble(txtroticanai.getText());
        itemcost[2] = Double.parseDouble(txtayampenyek.getText());
        itemcost[3] = Double.parseDouble(txtasampedas.getText());
        itemcost[4] = Double.parseDouble(txtasamboi.getText());
        itemcost[5] = Double.parseDouble(txtairkosong.getText());
        itemcost[6] = Double.parseDouble(txtairsirap.getText());
        itemcost[7] = Double.parseDouble(txtairlimau.getText());
        
        iSubTotal = itemcost[0] + itemcost[1] + itemcost[2]  + itemcost[3]  
                   + itemcost[4]  + itemcost[5]  + itemcost[6]  + itemcost[7] ;
        iTax = 0.06;
        String cTax = String.format("$%.2f",iTax);
        jlblTax.setText(cTax);
        
        String cSubTotal = String.format("$%.2f", iSubTotal);
        jlblsub.setText(cSubTotal);
                
        String iAmount = String.format("$%.2f", iSubTotal + iTax);
        jlblTotal.setText(iAmount);
        
    }//GEN-LAST:event_jButton1ActionPerformed

