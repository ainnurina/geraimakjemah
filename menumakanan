//nasilemak
    private void chknasilemakActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_chknasilemakActionPerformed
        // TODO add your handling code here:
        if (chknasilemak.isSelected())
        {
            txtnasilemak.setEnabled(true);
            txtnasilemak.requestFocus();
            txtnasilemak.setText("");
        }
        else
        {
            txtnasilemak.setEnabled(false);
            txtnasilemak.setText("0");
        }
    }//GEN-LAST:event_chknasilemakActionPerformed
